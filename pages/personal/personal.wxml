<!--personal.wxml-->

<view class="page">   
    <view class="section">   
            <image class="image" src="{{src}}" mode="{{mode}}"> </image>
            <view class="section-item bc_white">
                <view class ="section-item-item">
                    <view class="section-item-item-item">
                        <text>{{list}}</text>
                    </view>
                   
                    <view class="section-item-item-item">
                        <text> {{title}} </text>
                        <view class="section-item-item-item-color ">
                            <text> {{money}} </text>
                        </view>
                    </view>    
                </view>
            </view>       
    </view>
</view>


